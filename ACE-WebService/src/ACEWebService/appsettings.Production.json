{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "AppSettings": {
    "RabbitMQServer": "[RABBITMQ HOSTNAME]",
    "RabbitMQUserName": "[RABBITMQ USERNAME]",
    "RabbitMQPassword": "[RABBITMQ PASSWORD]",
    "EncryptionPassphrase": "[ENCRYPTION PASSPHRASE]",
    "Thumbprint": "[SSL THUMBPRINT]"
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=[SQL HOSTNAME];Database=ACEWebService;User Id=sa;Password=[SQL PASSWORD];MultipleActiveResultSets=true"
  }
}